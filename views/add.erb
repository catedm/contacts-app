<div class="container">
  <h1 class="header left black-text">Add Contact</h1>
  <div class="row">
    <form class="col s12" method="post" action="/add_contact">
      <div class="row">
        <div class="input-field col s6">
          <input id="first_name" type="text" class="validate" name="first_name" required>
          <label for="first_name">First Name</label>
        </div>
        <div class="input-field col s6">
          <input id="last_name" type="text" class="validate" name="last_name" required>
          <label for="last_name">Last Name</label>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s6">
          <input id="email" type="email" class="validate" name="email">
          <label for="email">Email</label>
        </div>
        <div class="input-field col s6">
          <input id="last_name" type="text" class="validate" name="phone">
          <label for="last_name">Phone</label>
        </div>
      </div>
      </div>
      <div class="row">
        <div class="input-field col s12">
          <input id="address" type="text" name="address">
          <label for="address">Address</label>
        </div>
      </div>
        <button class="btn waves-effect waves-light" type="submit">Add
          <i class="material-icons right">send</i>
        </button>
    </form>
  </div>
</div>
